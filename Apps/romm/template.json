{"version": "3", "templates": [    {
      "id": 172,
      "type": 3,
      "title": "RomM",
      "name": "romm",
      "description": "ROM manager for organizing, enriching, and playing retro games, with support for 400+ platforms.",
      "note": "RomM",
      "categories": ["BigBearCasaOS", "selfhosted"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/png/romm.png",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-portainer",
        "stackfile": "Apps/romm/docker-compose.yml"
      },
      "env": [
        {"name": "DB_HOST", "label": "DB_HOST", "description": "Database Host: romm_db", "default": ""},
        {"name": "DB_PORT", "label": "DB_PORT", "description": "Database Port: 3306", "default": ""},
        {"name": "DB_USER", "label": "DB_USER", "description": "Database User: romm-user", "default": ""},
        {"name": "DB_NAME", "label": "DB_NAME", "description": "Database Name: romm", "default": ""},
        {"name": "DB_PASSWD", "label": "DB_PASSWD", "description": "Database Password: casaos", "default": ""},
        {"name": "IGDB_CLIENT_ID", "label": "IGDB_CLIENT_ID", "description": "IGDB Client ID (Optional): Generate an ID and SECRET in IGDB", "default": ""},
        {"name": "IGDB_CLIENT_SECRET", "label": "IGDB_CLIENT_SECRET", "description": "IGDB Client Secret (Optional): https://api-docs.igdb.com/#account-creation", "default": ""},
        {"name": "MOBYGAMES_API_KEY", "label": "MOBYGAMES_API_KEY", "description": "MobyGames API Key (Optional): Generate at https://www.mobygames.com/info/api/", "default": ""},
        {"name": "STEAMGRIDDB_API_KEY", "label": "STEAMGRIDDB_API_KEY", "description": "SteamGridDB API Key (Optional): See https://github.com/rommapp/romm/wiki/Generate-API-Keys#steamgriddb", "default": ""},
        {"name": "SCREENSCRAPER_USER", "label": "SCREENSCRAPER_USER", "description": "ScreenScraper User (Optional): ScreenScraper username", "default": ""},
        {"name": "SCREENSCRAPER_PASSWORD", "label": "SCREENSCRAPER_PASSWORD", "description": "ScreenScraper Password (Optional): ScreenScraper password", "default": ""},
        {"name": "RETROACHIEVEMENTS_API_KEY", "label": "RETROACHIEVEMENTS_API_KEY", "description": "RetroAchievements API Key (Optional): RetroAchievements API key", "default": ""},
        {"name": "HASHEOUS_API_ENABLED", "label": "HASHEOUS_API_ENABLED", "description": "Hasheous API Enabled (Optional): Enable Hasheous API integration", "default": ""},
        {"name": "ROMM_AUTH_SECRET_KEY", "label": "ROMM_AUTH_SECRET_KEY", "description": "Authentication Secret Key: <secret key>", "default": ""},
        {"name": "ENABLE_RESCAN_ON_FILESYSTEM_CHANGE", "label": "ENABLE_RESCAN_ON_FILESYSTEM_CHANGE", "description": "Rescan On Filesystem Change: true", "default": ""},
        {"name": "ENABLE_SCHEDULED_RESCAN", "label": "ENABLE_SCHEDULED_RESCAN", "description": "Scheduled Rescan: true", "default": ""},
        {"name": "ENABLE_SCHEDULED_UPDATE_SWITCH_TITLEDB", "label": "ENABLE_SCHEDULED_UPDATE_SWITCH_TITLEDB", "description": "Scheduled Update Switch TitleDB: true", "default": ""}
      ]
    }]}
