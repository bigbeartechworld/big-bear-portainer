    {"name": "LIBREDESK_SYSTEM_USER_PASSWORD", "label": "LIBREDESK_SYSTEM_USER_PASSWORD", "description": "Environment variable for LIBREDESK_SYSTEM_USER_PASSWORD", "default": "c6b51a4e-ab9d-4f4f-933e-e96ed3570c1A"}
