{
  "version": "2",
  "templates": [
    {
      "id": 85,
      "type": 2,
      "title": "joplin",
      "name": "joplin",
      "description": "Note taking and to-do application with synchronisation",
      "note": "joplin",
      "categories": ["BigBearCasaOS", "selfhosted"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/png/joplin.png",
      "image": "joplin/server:3.4.3",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-casaos",
        "stackfile": "Apps/joplin/docker-compose.yml"
      },
      "env": [
    {"name": "APP_PORT", "label": "APP_PORT", "description": "Environment variable for APP_PORT", "default": "22300"},
    {"name": "APP_BASE_URL", "label": "APP_BASE_URL", "description": "Environment variable for APP_BASE_URL", "default": "http://localhost:22300"},
    {"name": "DB_CLIENT", "label": "DB_CLIENT", "description": "Environment variable for DB_CLIENT", "default": "pg"},
    {"name": "POSTGRES_PASSWORD", "label": "POSTGRES_PASSWORD", "description": "Environment variable for POSTGRES_PASSWORD", "default": "27a1d42a-a15d-43dd-903c-2a73538647fe"},
    {"name": "POSTGRES_USER", "label": "POSTGRES_USER", "description": "Environment variable for POSTGRES_USER", "default": "bigbear"},
    {"name": "POSTGRES_DATABASE", "label": "POSTGRES_DATABASE", "description": "Environment variable for POSTGRES_DATABASE", "default": "joplin"},
    {"name": "POSTGRES_PORT", "label": "POSTGRES_PORT", "description": "Environment variable for POSTGRES_PORT", "default": "5432"},
    {"name": "POSTGRES_HOST", "label": "POSTGRES_HOST", "description": "Environment variable for POSTGRES_HOST", "default": "big-bear-joplin-db"},
    {"name": "MAX_TIME_DRIFT", "label": "MAX_TIME_DRIFT", "description": "Environment variable for MAX_TIME_DRIFT", "default": "0"},
    {"name": "POSTGRES_PASSWORD", "label": "POSTGRES_PASSWORD", "description": "Environment variable for POSTGRES_PASSWORD", "default": "27a1d42a-a15d-43dd-903c-2a73538647fe"},
    {"name": "POSTGRES_USER", "label": "POSTGRES_USER", "description": "Environment variable for POSTGRES_USER", "default": "bigbear"},
    {"name": "POSTGRES_DB", "label": "POSTGRES_DB", "description": "Environment variable for POSTGRES_DB", "default": "joplin"}
      ],
      "ports": [    "22300:22300"],
      "volumes": [
    {"container": "/var/lib/postgresql/data"}
      ],
      "labels": [
        {"name": "maintainer", "value": "BigBearTechWorld"},
        {"name": "version", "value": "3.4.3"},
        {"name": "source", "value": "big-bear-casaos"}
      ],
      "administrator_only": false
    }
  ]
}
