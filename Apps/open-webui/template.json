      "image": "ghcr.io/open-webui/open-webui:git-33e54a9",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-casaos",
        "stackfile": "Apps/open-webui/docker-compose.yml"
      },
      "env": [
    {"name": "OLLAMA_BASE_URL", "label": "OLLAMA_BASE_URL", "description": "Environment variable for OLLAMA_BASE_URL", "default": ""},
