{
  "version": "2",
  "templates": [
    {
      "id": 200,
      "type": 2,
      "title": "Warracker",
      "name": "warracker",
      "description": "A self-hosted warranty tracker",
      "note": "A self-hosted warranty tracker",
      "categories": ["BigBearCasaOS", "selfhosted"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/png/warracker.png",
      "image": "ghcr.io/sassanix/warracker/main:9.9.3",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-casaos",
        "stackfile": "Apps/warracker/docker-compose.yml"
      },
      "env": [
    {"name": "DB_HOST", "label": "DB_HOST", "description": "Environment variable for DB_HOST", "default": "big-bear-warrackerdb"},
    {"name": "DB_NAME", "label": "DB_NAME", "description": "Environment variable for DB_NAME", "default": "big_bear_warracker"},
    {"name": "DB_USER", "label": "DB_USER", "description": "Environment variable for DB_USER", "default": "big_bear_warracker_user"},
    {"name": "DB_PASSWORD", "label": "DB_PASSWORD", "description": "Environment variable for DB_PASSWORD", "default": "4892f1bc-f1ea-464f-b528-1d52b2c9bb05"},
    {"name": "SMTP_HOST", "label": "SMTP_HOST", "description": "Environment variable for SMTP_HOST", "default": "smtp.email.com"},
    {"name": "SMTP_PORT", "label": "SMTP_PORT", "description": "Environment variable for SMTP_PORT", "default": "465"},
    {"name": "SMTP_USERNAME", "label": "SMTP_USERNAME", "description": "Environment variable for SMTP_USERNAME", "default": "youremail@email.com"},
    {"name": "SMTP_PASSWORD", "label": "SMTP_PASSWORD", "description": "Environment variable for SMTP_PASSWORD", "default": "password"},
    {"name": "POSTGRES_DB", "label": "POSTGRES_DB", "description": "Environment variable for POSTGRES_DB", "default": "big_bear_warracker"},
    {"name": "POSTGRES_USER", "label": "POSTGRES_USER", "description": "Environment variable for POSTGRES_USER", "default": "big_bear_warracker_user"},
    {"name": "POSTGRES_PASSWORD", "label": "POSTGRES_PASSWORD", "description": "Environment variable for POSTGRES_PASSWORD", "default": "4892f1bc-f1ea-464f-b528-1d52b2c9bb05"}
      ],
      "ports": [    "8005:80"],
      "volumes": [
    {"container": "/data/uploads"},
    {"container": "/var/lib/postgresql/data"}
      ],
      "labels": [
        {"name": "maintainer", "value": "BigBearTechWorld"},
        {"name": "version", "value": "9.9.3"},
        {"name": "source", "value": "big-bear-casaos"}
      ],
      "administrator_only": false
    }
  ]
}
