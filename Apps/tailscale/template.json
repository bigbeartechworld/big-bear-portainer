{
  "version": "3",
  "templates": [
    {
      "id": 181,
      "type": 2,
      "title": "Tailscale",
      "name": "tailscale",
      "description": "Zero config VPN. Installs on any device in minutes, manages firewall rules for you, and works from anywhere.",
      "note": "The easiest, most secure way to use WireGuard and 2FA.",
      "categories": ["BigBearCasaOS", "selfhosted"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/png/tailscale.png",
      "image": "tailscale/tailscale:v1.88.3",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-casaos",
        "stackfile": "Apps/tailscale/docker-compose.yml"
      },
      "env": [
    {"name": "TS_SERVE_CONFIG", "label": "TS_SERVE_CONFIG", "description": "Environment variable for TS_SERVE_CONFIG", "default": ""},
    {"name": "TS_ACCEPT_DNS", "label": "TS_ACCEPT_DNS", "description": "Environment variable for TS_ACCEPT_DNS", "default": "false"},
    {"name": "TS_AUTH_ONCE", "label": "TS_AUTH_ONCE", "description": "Environment variable for TS_AUTH_ONCE", "default": "false"},
    {"name": "TS_AUTHKEY", "label": "TS_AUTHKEY", "description": "Environment variable for TS_AUTHKEY", "default": ""},
    {"name": "TS_HOSTNAME", "label": "TS_HOSTNAME", "description": "Environment variable for TS_HOSTNAME", "default": ""},
    {"name": "TS_ROUTES", "label": "TS_ROUTES", "description": "Environment variable for TS_ROUTES", "default": ""},
    {"name": "TS_EXTRA_ARGS", "label": "TS_EXTRA_ARGS", "description": "Environment variable for TS_EXTRA_ARGS", "default": ""},
    {"name": "TS_USERSPACE", "label": "TS_USERSPACE", "description": "Environment variable for TS_USERSPACE", "default": "true"},
    {"name": "TS_STATE_DIR", "label": "TS_STATE_DIR", "description": "Environment variable for TS_STATE_DIR", "default": "/var/lib/tailscale"}
      ],
      "volumes": [
    {"container": "/var/lib/tailscale"},
    {"container": "/config"},
    {"container": "/dev/net/tun"}
      ],
      "labels": [
        {"name": "maintainer", "value": "BigBearTechWorld"},
        {"name": "version", "value": "1.88.3"},
        {"name": "source", "value": "big-bear-casaos"}
      ],
      "administrator_only": false
    }
  ]
}
