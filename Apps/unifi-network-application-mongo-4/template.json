{
  "version": "2",
  "templates": [
    {
      "id": 193,
      "type": 2,
      "title": "Unifi Network Application",
      "name": "unifi-network-application-mongo-4",
      "description": "Supports Zimaboards and PI's. The Unifi-network-application software is a powerful, enterprise wireless software engine ideal for high-density client deployments requiring low latency and high uptime performance.",
      "note": "The Unifi-network-application MongoDB 4",
      "categories": ["BigBearCasaOS", "selfhosted"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/png/ubiquiti-unifi.png",
      "image": "mongo:4.4.28",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-casaos",
        "stackfile": "Apps/unifi-network-application-mongo-4/docker-compose.yml"
      },
      "env": [
    {"name": "PUID", "label": "PUID", "description": "Environment variable for PUID", "default": "1000"},
    {"name": "PGID", "label": "PGID", "description": "Environment variable for PGID", "default": "1000"},
    {"name": "TZ", "label": "TZ", "description": "Environment variable for TZ", "default": "Etc/UTC"},
    {"name": "MONGO_HOST", "label": "MONGO_HOST", "description": "Environment variable for MONGO_HOST", "default": "big-bear-unifi-db"},
    {"name": "MONGO_USER", "label": "MONGO_USER", "description": "Environment variable for MONGO_USER", "default": "unifi"},
    {"name": "MONGO_PASS", "label": "MONGO_PASS", "description": "Environment variable for MONGO_PASS", "default": "unifi_password"},
    {"name": "MONGO_PORT", "label": "MONGO_PORT", "description": "Environment variable for MONGO_PORT", "default": "27017"},
    {"name": "MONGO_DBNAME", "label": "MONGO_DBNAME", "description": "Environment variable for MONGO_DBNAME", "default": "unifi"},
    {"name": "MEM_LIMIT", "label": "MEM_LIMIT", "description": "Environment variable for MEM_LIMIT", "default": "1024"},
    {"name": "MEM_STARTUP", "label": "MEM_STARTUP", "description": "Environment variable for MEM_STARTUP", "default": "1024"},
    {"name": "MONGO_TLS", "label": "MONGO_TLS", "description": "Environment variable for MONGO_TLS", "default": ""},
    {"name": "MONGO_AUTHSOURCE", "label": "MONGO_AUTHSOURCE", "description": "Environment variable for MONGO_AUTHSOURCE", "default": ""}
      ],
      "ports": [    "27017:27017",    "8443:8443",    "3478:3478/udp",    "10001:10001/udp",    "8080:8080",    "1900:1900/udp",    "8843:8843",    "8880:8880",    "6789:6789",    "5514:5514/udp"],
      "volumes": [
    {"container": "/data/db"},
    {"container": "/docker-entrypoint-initdb.d/init-mongo.js"},
    {"container": "/config"}
      ],
      "labels": [
        {"name": "maintainer", "value": "BigBearTechWorld"},
        {"name": "version", "value": "4.4.28"},
        {"name": "source", "value": "big-bear-casaos"}
      ],
      "administrator_only": false
    }
  ]
