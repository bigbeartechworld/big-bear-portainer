{
  "version": "2",
  "templates": [
    {
      "id": 175,
      "type": 2,
      "title": "SFTPGO",
      "name": "sftpgo",
      "description": "Full-featured and highly configurable SFTP, HTTP/S, FTP/S and WebDAV server - S3, Google Cloud Storage, Azure Blob",
      "note": "SFTPGO",
      "categories": ["BigBearCasaOS", "selfhosted"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/png/sftpgo.png",
      "image": "drakkan/sftpgo:v2.6",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-casaos",
        "stackfile": "Apps/sftpgo/docker-compose.yml"
      },
      "env": [
    {"name": "SFTPGO_HTTPD_BINDINGS_0_PORT", "label": "SFTPGO_HTTPD_BINDINGS_0_PORT", "description": "Environment variable for SFTPGO_HTTPD_BINDINGS_0_PORT", "default": "8080"},
    {"name": "SFTPGO_HTTPD_BINDINGS_0_ADDRESS", "label": "SFTPGO_HTTPD_BINDINGS_0_ADDRESS", "description": "Environment variable for SFTPGO_HTTPD_BINDINGS_0_ADDRESS", "default": "0.0.0.0"},
    {"name": "SFTPGO_DATA_PROVIDER_CREATE_DEFAULT_ADMIN", "label": "SFTPGO_DATA_PROVIDER_CREATE_DEFAULT_ADMIN", "description": "Environment variable for SFTPGO_DATA_PROVIDER_CREATE_DEFAULT_ADMIN", "default": "true"},
    {"name": "SFTPGO_WEBDAVD_BINDINGS_0_PORT", "label": "SFTPGO_WEBDAVD_BINDINGS_0_PORT", "description": "Environment variable for SFTPGO_WEBDAVD_BINDINGS_0_PORT", "default": "10080"},
    {"name": "SFTPGO_FTPD_BINDINGS_0_PORT", "label": "SFTPGO_FTPD_BINDINGS_0_PORT", "description": "Environment variable for SFTPGO_FTPD_BINDINGS_0_PORT", "default": "2121"},
    {"name": "SFTPGO_FTPD_BINDINGS_0_FORCE_PASSIVE_IP", "label": "SFTPGO_FTPD_BINDINGS_0_FORCE_PASSIVE_IP", "description": "Environment variable for SFTPGO_FTPD_BINDINGS_0_FORCE_PASSIVE_IP", "default": "'127.0.0.1:8080'"}
      ],
      "ports": [    "8080:8080",    "2022:2022",    "2121:2121",    "10080:10080"],
      "volumes": [
    {"container": "/srv/sftpgo"},
    {"container": "/var/lib/sftpgo"}
      ],
      "labels": [
        {"name": "maintainer", "value": "BigBearTechWorld"},
        {"name": "version", "value": "v2.6"},
        {"name": "source", "value": "big-bear-casaos"}
      ],
      "administrator_only": false
    }
  ]
