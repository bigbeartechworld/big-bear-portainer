{
  "version": "3",
  "templates": [
    {
      "id": 127,
      "type": 2,
      "title": "Obsidian",
      "name": "obsidian",
      "description": "Obsidian is a note-taking app that lets you create, link, and organize your notes on your device, with hundreds of plugins and themes to customize your workflow. You can also publish your notes online, access them offline, and sync them securely with end-to-end encryption.",
      "note": "Obsidian",
      "categories": ["BigBearCasaOS", "selfhosted"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/png/obsidian.png",
      "image": "linuxserver/obsidian:1.9.14",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-casaos",
        "stackfile": "Apps/obsidian/docker-compose.yml"
      },
      "env": [
    {"name": "PUID", "label": "PUID", "description": "Environment variable for PUID", "default": "1000"},
    {"name": "PGID", "label": "PGID", "description": "Environment variable for PGID", "default": "1000"},
    {"name": "TZ", "label": "TZ", "description": "Environment variable for TZ", "default": "Etc/UTC"}
      ],
      "ports": [    "3000:3000",    "3001:3001"],
      "volumes": [
    {"container": "/config"}
      ],
      "labels": [
        {"name": "maintainer", "value": "BigBearTechWorld"},
        {"name": "version", "value": "1.9.14"},
        {"name": "source", "value": "big-bear-casaos"}
      ],
      "administrator_only": false
    }
  ]
}
