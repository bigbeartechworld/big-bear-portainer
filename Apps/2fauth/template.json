        "stackfile": "Apps/2fauth/docker-compose.yml"
      },
      "env": [
    {"name": "APP_URL", "label": "APP_URL", "description": "Environment variable for APP_URL", "default": "http://[CHANGE_TO_LAN_IP]:8000"}
