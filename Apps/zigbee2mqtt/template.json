      "categories": ["BigBearCasaOS", "selfhosted"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/png/zigbee2mqtt.png",
      "image": "koenkk/zigbee2mqtt:2.6.2",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-casaos",
        "stackfile": "Apps/zigbee2mqtt/docker-compose.yml"
      },
      "env": [
    {"name": "TZ", "label": "TZ", "description": "Environment variable for TZ", "default": "UTC"}
