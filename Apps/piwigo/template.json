{
  "version": "3",
  "templates": [
    {
      "id": 149,
      "type": 2,
      "title": "Piwigo",
      "name": "piwigo",
      "description": "Piwigo is a photo gallery software for the web that comes with powerful features to publish and manage your collection of pictures.",
      "note": "Piwigo",
      "categories": ["BigBearCasaOS", "selfhosted"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/png/piwigo.png",
      "image": "linuxserver/piwigo:15.6.0",
      "repository": {
        "url": "https://github.com/bigbeartechworld/big-bear-casaos",
        "stackfile": "Apps/piwigo/docker-compose.yml"
      },
      "env": [
    {"name": "PUID", "label": "PUID", "description": "Environment variable for PUID", "default": "1000"},
    {"name": "PGID", "label": "PGID", "description": "Environment variable for PGID", "default": "1000"},
    {"name": "TZ", "label": "TZ", "description": "Environment variable for TZ", "default": "UTC"},
    {"name": "MYSQL_ROOT_PASSWORD", "label": "MYSQL_ROOT_PASSWORD", "description": "Environment variable for MYSQL_ROOT_PASSWORD", "default": "3cf3f2c4-3415-4efe-b478-db7b6f0b9934"},
    {"name": "MYSQL_DATABASE", "label": "MYSQL_DATABASE", "description": "Environment variable for MYSQL_DATABASE", "default": "piwigo"},
    {"name": "MYSQL_USER", "label": "MYSQL_USER", "description": "Environment variable for MYSQL_USER", "default": "big_bear_piwigo"},
    {"name": "MYSQL_PASSWORD", "label": "MYSQL_PASSWORD", "description": "Environment variable for MYSQL_PASSWORD", "default": "358fac48-4beb-4027-9c11-840464aca400"}
      ],
      "ports": [    "8080:80"],
      "volumes": [
    {"container": "/config"},
    {"container": "/gallery"},
    {"container": "/var/lib/mysql"}
      ],
      "labels": [
        {"name": "maintainer", "value": "BigBearTechWorld"},
        {"name": "version", "value": "15.6.0"},
        {"name": "source", "value": "big-bear-casaos"}
      ],
      "administrator_only": false
    }
  ]
}
